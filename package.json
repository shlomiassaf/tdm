    "ts-node": "^2.1.0",
    "tslib": "^1.5.0",
    "tslint": "^4.4.2",
    "typedoc": "^0.5.5",
    "typescript": "next",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
