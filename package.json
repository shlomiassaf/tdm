    "assets-webpack-plugin": "^3.5.1",
    "awesome-typescript-loader": "^3.0.3",
    "blueimp-tmpl": "^3.7.0",
    "circular-dependency-plugin": "^2.0.0",
    "codelyzer": "^2.0.0",
