{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@tdm/core": [
        "libs/core/src/index.ts"
      ],
      "@tdm/core/lib/*": [
        "libs/core/src/lib/*"
      ],
      "@tdm/core/tdm": [
        "libs/core/tdm/src/index.ts"
      ],
      "@tdm/core/tdm/lib/*": [
        "libs/core/tdm/src/lib/*"
      ],
      "@tdm/core/testing": [
        "libs/core/testing/src/index.ts"
      ],
      "@tdm/core/testing/lib/*": [
        "libs/core/testing/src/lib/*"
      ],
      "@tdm/tixin": [
        "libs/tixin/src/index.ts"
      ],
      "@tdm/tixin/lib/*": [
        "libs/tixin/src/lib/*"
      ],
      "@tdm/data": [
        "libs/data/src/index.ts"
      ],
      "@tdm/data/lib/*": [
        "libs/data/src/lib/*"
      ],
      "@tdm/data/plugin/active-record": [
        "libs/data/plugin/active-record/src/index.ts"
      ],
      "@tdm/data/plugin/active-record/lib/*": [
        "libs/data/plugin/active-record/src/lib/*"
      ],
      "@tdm/data/plugin/control-flow": [
        "libs/data/plugin/control-flow/src/index.ts"
      ],
      "@tdm/data/plugin/control-flow/lib/*": [
        "libs/data/plugin/control-flow/src/lib/*"
      ],
      "@tdm/data/plugin/rx-resource-control": [
        "libs/data/plugin/rx-resource-control/src/index.ts"
      ],
      "@tdm/data/plugin/rx-resource-control/lib/*": [
        "libs/data/plugin/rx-resource-control/src/lib/*"
      ],
      "@tdm/data/testing": [
        "libs/data/testing/src/index.ts"
      ],
      "@tdm/data/testing/lib/*": [
        "libs/data/testing/src/lib/*"
      ],
      "@tdm/ngx-dynamic-forms": [
        "libs/ngx-dynamic-forms/src/index.ts"
      ],
      "@tdm/ngx-dynamic-forms/lib/*": [
        "libs/ngx-dynamic-forms/src/lib/*"
      ],
      "@tdm/ngx-dynamic-forms/plugin/material": [
        "libs/ngx-dynamic-forms/plugin/material/src/index.ts"
      ],
      "@tdm/ngx-dynamic-forms/plugin/material/lib/*": [
        "libs/ngx-dynamic-forms/plugin/material/src/lib/*"
      ],
      "@tdm/json-api-mapper": [
        "libs/json-api-mapper/src/index.ts"
      ],
      "@tdm/json-api-mapper/lib/*": [
        "libs/json-api-mapper/src/lib/*"
      ],
      "@tdm/ngx-http-client": [
        "libs/ngx-http-client/src/index.ts"
      ],
      "@tdm/ngx-http-client/lib/*": [
        "libs/ngx-http-client/src/lib/*"
      ],
      "@tdm/local-forage": [
        "libs/local-forage/src/index.ts"
      ],
      "@tdm/local-forage/lib/*": [
        "libs/local-forage/src/lib/*"
      ],
      "@tdm/service-mocker": [
        "libs/service-mocker/src/index.ts"
      ],
      "@tdm/service-mocker/lib/*": [
        "libs/service-mocker/src/lib/*"
      ],
      "@tdm/service-mocker/shared": [
        "libs/service-mocker/shared/src/index.ts"
      ],
      "@tdm/service-mocker/shared/lib/*": [
        "libs/service-mocker/shared/src/lib/*"
      ],
      "@tdm/service-mocker/client": [
        "libs/service-mocker/client/src/index.ts"
      ],
      "@tdm/service-mocker/client/lib/*": [
        "libs/service-mocker/client/src/lib/*"
      ],
      "@app": [
        "apps/demo/src/modules/@app"
      ],
      "@forms": [
        "apps/demo/src/modules/@forms"
      ],
      "@forms/shared": [
        "apps/demo/src/modules/@forms/shared"
      ],
      "@http": [
        "apps/demo/src/modules/@http"
      ],
      "@http/shared": [
        "apps/demo/src/modules/@http/shared"
      ],
      "@northwind-app": [
        "apps/demo/src/modules/@northwind-app"
      ],
      "@shared": [
        "apps/demo/src/modules/@shared"
      ],
      "@shared/client": [
        "apps/demo/src/modules/@shared/client"
      ],
      "server": [
        "apps/demo/src/modules/server"
      ],
      "@webpack-ext/*": [
        "tools/@webpack-ext/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "tmp"
  ]
}